{
  "name": "@example/dotstar-node-server",
  "main": "dist/index.js",
  "version": "0.0.0",
  "scripts": {
    "build": "rimraf dist && tsc -p src",
    "start": "nodemon -w src --exec \"npm run build && node dist\"",
    "dev": "nodemon -w dist --delay 1 --exec \"node dist\"",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "dotstar-node": "file:../..",
    "express": "^4.16.4",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.2",
    "@types/ramda": "^0.26.0",
    "@types/ws": "^6.0.1",
    "nodemon": "^1.18.5",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.6"
  }
}
