{
  "name": "dotstar-node",
  "version": "1.0.0",
  "description": "Interacting with Dotstar LEDs via Node.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc -p src",
    "build:all": "npm run build && (cd example/server && npm run build) && (cd example/dotstar-app && npm run build)",
    "sync-build": "nodemon --config nodemon.build-sync.config.json",
    "sync-gyp": "nodemon --config nodemon.gyp-sync.config.json",
    "sync-changes": "nodemon --config nodemon.change-sync.config.json",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeden/dotstar-node.git"
  },
  "author": "Alex Eden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexeden/dotstar-node/issues"
  },
  "homepage": "https://github.com/alexeden/dotstar-node#readme",
  "devDependencies": {
    "@types/node": "^10.12.2",
    "@types/ramda": "^0.26.0",
    "nodemon": "^1.18.5",
    "rimraf": "^2.6.2",
    "rsync": "^0.6.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "gradient-string": "^1.2.0",
    "spi-node": "^1.0.0"
  }
}
