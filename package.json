{
  "name": "dotstar-node",
  "version": "1.0.0",
  "description": "Interacting with Dotstar LEDs via Node.js",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "remote": "nodemon -e js,json --watch dist dist/index.js",
    "clean": "rimraf dist",
    "watch": "nodemon --exec \"npm run start\"",
    "server-watch": "nodemon --exec \"npm run server\"",
    "server": "node --es_staging --strong_this --harmony_modules --harmony_sloppy_let --harmony_default_parameters dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexeden/dotstar-node.git"
  },
  "author": "Alex Eden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexeden/dotstar-node/issues"
  },
  "homepage": "https://github.com/alexeden/dotstar-node#readme",
  "devDependencies": {
    "@types/node": "0.0.2",
    "@types/pi-spi": "0.0.27",
    "@types/ramda": "0.0.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chalk": "^1.1.1",
    "gulp": "git://github.com/gulpjs/gulp.git#d8f5c90a0622d19ef1943a2a3d02dc50e3c853e7",
    "gulp-debug": "^3.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-typescript": "^3.1.3",
    "nodemon": "^1.11.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.2.0-dev.20161207"
  },
  "dependencies": {
    "pi-spi": "^1.0.1",
    "ramda": "^0.22.1",
    "rxjs": "^5.0.0-rc.5"
  }
}
